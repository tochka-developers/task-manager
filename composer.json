{
  "name": "tochka-developers/task-manager",
  "description": "Task manager for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Babushkin",
      "email": "darkdarin@tochka.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "illuminate/support": "5.7.*|5.8.*|^6.0",
    "illuminate/container": "5.7.*|5.8.*|^6.0",
    "illuminate/database": "5.7.*|5.8.*|^6.0",
    "illuminate/console": "5.7.*|5.8.*|^6.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Tochka\\TaskManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tochka\\TaskManager\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tochka\\TaskManager\\TaskManagerServiceProvider"
      ]
    }
  }
}
